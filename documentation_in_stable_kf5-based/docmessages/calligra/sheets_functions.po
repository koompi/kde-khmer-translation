#: functions.docbook:9670
#, no-c-format
msgid "<link linkend=\"ASC\">ASC</link>&nbsp;"
msgstr "<link linkend=\"ASC\"> ASC </link>&nbsp;"

#. Tag: title
#: functions.docbook:9677
#, no-c-format
msgid "LEFT"
msgstr "LEFT"

#. Tag: para
#: functions.docbook:9678
#, no-c-format
msgid "The LEFT() function returns a substring that contains the 'length' leftmost characters of the string. The whole string is returned if 'length' exceeds the length of the string. It is an error for the number of characters to be less than 0."
msgstr "អនុគមន៍ LEFT() ត្រឡប់ខ្សែអក្សររងដែលមានតួអក្សរខាងឆ្វេងបំផុតនៃ 'ខ្សែអក្សរ' ។ ខ្សែអក្សរទាំងមូលត្រូវបានត្រឡប់មកវិញប្រសិនបើ 'ប្រវែង' លើសពីប្រវែងនៃខ្សែអក្សរ។ វាជាកំហុសមួយសម្រាប់ចំនួនតួអក្សរតិចជាង ០ ។"

#. Tag: para
#: functions.docbook:9683
#, no-c-format
msgid "LEFT(text;length)"
msgstr "LEFT (អត្ថបទប្រវែង)"

#. Tag: seg
#: functions.docbook:9688 functions.docbook:10029
#, no-c-format
msgid "Number of characters"
msgstr "ចំនួនតួអក្សរ"

#. Tag: para
#: functions.docbook:9691
#, no-c-format
msgid "LEFT(\"hello\";2) returns \"he\""
msgstr "LEFT (\"hello\"; ២) ត្រឡប់ \"he\""

#. Tag: para
#: functions.docbook:9694
#, no-c-format
msgid "LEFT(\"KSpread\";10) returns \"KSpread\""
msgstr "LEFT (\"KSpread\"; ១០) ត្រឡប់ \"KSpread\""

#. Tag: para
#: functions.docbook:9697
#, no-c-format
msgid "LEFT(\"KSpread\") returns \"K\""
msgstr "LEFT (\"KSpread\") ត្រឡប់ \"K\""

#. Tag: member
#: functions.docbook:9701 functions.docbook:9726 functions.docbook:9830
#: functions.docbook:9857
#, no-c-format
msgid "<link linkend=\"RIGHT\">RIGHT</link>&nbsp;"
msgstr "<link linkend=\"RIGHT\"> RIGHT </link>&nbsp;"

#. Tag: member
