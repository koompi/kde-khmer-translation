#: index.docbook:43
#, fuzzy, no-c-format
msgid "<command>man:/</command><replaceable>fopen(3)</replaceable>"
msgstr "<command>man:</command>fopen"

#. Tag: para
#: index.docbook:45
#, fuzzy, no-c-format
msgid "See the man page of <command>fopen</command> in section 3."
msgstr "See the man page of <command>fopen</command>."

#. Tag: userinput
#: index.docbook:50
#, fuzzy, no-c-format
msgid "<command>man:/</command><replaceable>(3)</replaceable>"
msgstr "<command>man:</command>fopen"

#. Tag: para
#: index.docbook:52
#, no-c-format
msgid "See the index of section 3."
msgstr ""

#. Tag: userinput
#: index.docbook:57
#, fuzzy, no-c-format
