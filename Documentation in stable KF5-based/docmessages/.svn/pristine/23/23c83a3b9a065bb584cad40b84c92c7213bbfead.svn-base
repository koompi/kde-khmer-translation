#. Tag: para
#: index.docbook:63
#, no-c-format
msgid "<guilabel>Menu:</guilabel> Font used in &kde; application menus"
msgstr "<guilabel>Menu:</guilabel> Font used in &kde; application menus"

#. Tag: para
#: index.docbook:66
#, no-c-format
msgid "<guilabel>Window title:</guilabel> Font used in the window title"
