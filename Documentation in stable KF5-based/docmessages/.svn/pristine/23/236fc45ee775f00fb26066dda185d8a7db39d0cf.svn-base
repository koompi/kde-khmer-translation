#, no-c-format
msgid ""
"The command used to run the diff program (default <application>diff</"
"application>)."
msgstr ""

#. Tag: title
#: index.docbook:507
#, no-c-format
msgid "Format"
msgstr ""

#. Tag: screeninfo
#: index.docbook:509
#, no-c-format
msgid "<screeninfo>&kompare; Format Settings</screeninfo>"
msgstr ""

#. Tag: phrase
#: index.docbook:515
#, no-c-format
